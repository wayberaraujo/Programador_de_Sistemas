<script>
    var nome, sobrenome,nomeesobrenome;
        primeironome = prompt("digite o nome");
        segundosobrenome = prompt("digite o sobrenome");
        nomeesobrenome = primeironome + segundosobrenome;
        document.write("o resultado da junção:" + primeironome + "e" + segundosobrenome + " é " + nomeesobrenome)</script> 